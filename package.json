{
    "name": "daily-commit",
    "version": "1.0.0",
    "main": "dist/index.js",
    "author": "Jacob Andes",
    "license": "MIT",
    "private": true,
    "scripts": {
        "build": "tsc",
        "build:optimized": "tsc && cp dist/indexOptimized.js dist/index.js",
        "dev": "ts-node src/index.ts",
        "dev:optimized": "ts-node src/indexOptimized.ts",
        "package": "yarn build && zip -r lambda-deployment.zip dist/ node_modules/",
        "package:optimized": "yarn build:optimized && zip -r lambda-deployment-optimized.zip dist/ node_modules/",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "tsc --noEmit",
        "health": "ts-node src/health.ts",
        "analyze": "yarn build && du -sh dist/ && du -sh node_modules/",
        "analyze:optimized": "yarn build:optimized && du -sh dist/ && du -sh node_modules/"
    },
    "dependencies": {
        "@aws-sdk/client-cloudwatch": "^3.0.0",
        "@aws-sdk/client-lambda": "^3.0.0",
        "@aws-sdk/client-secrets-manager": "^3.0.0"
    },
    "devDependencies": {
        "@jest/globals": "^29.0.0",
        "@types/aws-lambda": "^8.10.145",
        "@types/jest": "^29.0.0",
        "@types/node": "^20.0.0",
        "jest": "^29.0.0",
        "ts-jest": "^29.0.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.2"
    }
}
